
<div class="Header" style="display: block;">
    <a href="/">Home</a>
</div>
<h2 class="ta-center">SEP/CRUD Framework</h2>
<table id="tour">
    <tr>
        <th>STT</th>
        <th>TableName</th>
        <th>Action</th>
    </tr>
    {{#each allTable}}
    <tr>
        <td>{{@index}}</td>
        <td>{{this}}</td>
        <td> <button class="btn btn-outline-secondary" onclick="myFunc('{{this}}')">Go</button>
        </td>
    </tr>
    {{/each}}
</table>
<script>
    myFunc = (tableName) => {
        window.location.replace(`/table-detail/${tableName}`);
    }
</script>